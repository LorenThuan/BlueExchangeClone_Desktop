	<queryString language="SQL">
		<![CDATA[SELECT  Chi_Tiet_Hoa_Don.maHoaDon, ngayDat, Nhan_Vien.tenNhanVien, Khach_Hang.tenKhachHang, Khach_Hang.soDienThoai, San_Pham.maSanPham, San_Pham.tenSanPham, San_Pham.kichThuoc, San_Pham.mauSac, San_Pham.donGia, Chi_Tiet_Hoa_Don.soLuong, sum(Chi_Tiet_Hoa_Don.[soLuong] * [donGia] * (1 - San_Pham.[giamGia])) * 1.1 AS ThanhTien    
FROM            Chi_Tiet_Hoa_Don INNER JOIN
                         Hoa_Don ON Chi_Tiet_Hoa_Don.maHoaDon = Hoa_Don.maHoaDon INNER JOIN
                         San_Pham ON Chi_Tiet_Hoa_Don.maSanPham = San_Pham.maSanPham INNER JOIN
                         Khach_Hang ON Hoa_Don.maKhachHang = Khach_Hang.maKhachHang INNER JOIN
                         Nhan_Vien ON Hoa_Don.maNhanVien = Nhan_Vien.maNhanVien
WHERE Nhan_Vien.maNhanVien = $P{maNhanVien} and ngayDat =  $P{ngayHomNay} and Nhan_Vien.trangThai = 1
GROUP BY Chi_Tiet_Hoa_Don.maHoaDon, ngayDat, Nhan_Vien.tenNhanVien, Khach_Hang.tenKhachHang, Khach_Hang.soDienThoai, San_Pham.maSanPham, San_Pham.tenSanPham, San_Pham.kichThuoc, San_Pham.mauSac, San_Pham.donGia, Chi_Tiet_Hoa_Don.soLuong, San_Pham.[giamGia]]]>
	</queryString>
